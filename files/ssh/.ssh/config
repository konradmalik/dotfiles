Include config.d/*

AddKeysToAgent yes
TCPKeepAlive yes
ServerAliveInterval 15
ServerAliveCountMax 6
Compression yes
ControlMaster auto
ControlPath /tmp/%r@%h:%p
ControlPersist 300

# fix pinetry for gpg agent
Match host * exec "gpg-connect-agent UPDATESTARTUPTTY /bye"

# #########
# Safety measures
# Keep this at the very end
# #########
Host *
  PubkeyAuthentication no
  IdentitiesOnly yes

