Host github.com
  PubkeyAuthentication yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/github
  User git

Host gitlab.com
  PubkeyAuthentication yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/bitbucket
  User git

Host bitbucket.org
  PubkeyAuthentication yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/gitlab
  User git
